{
  "name": "vuestro",
  "version": "2.17.2",
  "description": "Vue.js 2 Components",
  "author": "Mike Smiley <msmiley@alumni.rice.edu>",
  "url": "https://github.com/msmiley/vuestro",
  "repository": "msmiley/vuestro.git",
  "license": "ISC",
  "main": "./src/plugin/index.js",
  "scripts": {
    "dev": "webpack-dev-server -d --mode development --hot --config ./webpack.config.js",
    "build": "webpack --mode production --config ./webpack.config.js",
    "test": "jest --verbose --coverage"
  },
  "dependencies": {
    "brace": "^0.11.1",
    "d3": "^5.9.2",
    "d3-sankey": "^0.12.1",
    "geopattern": "^1.2.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "vue-awesome": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "url-loader": "^1.1.2",
    "vue": "^2.5.22",
    "vue-jest": "^3.0.2",
    "vue-loader": "^15.5.1",
    "vue-router": "^3.0.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(vue-awesome)/)"
    ]
  },
  "keywords": [
    "Vue",
    "gui",
    "ux",
    "framework",
    "widgets",
    "ui"
  ]
}
